      </div>

    </div><!-- #page-menu end -->

    <!-- Content
		============================================= -->
    <section id="content">

			<div class="content-wrap">

				<div class="container clearfix">

					<div class="tabs divcenter nobottommargin clearfix" id="tab-login-register" style="max-width: 500px;">


						<div class="tab-container">

							<div class="tab-content clearfix" id="tab-login">
								<div class="card nobottommargin">
									<div class="card-body" style="padding: 40px;">
										<form id="login-form" name="login-form" class="nobottommargin">

											<h3>Connecter vous Ã  votre compte</h3>

											<div class="col_full">
												<label for="login-form-username">Mail:</label>
												<input type="mail" id="login-form-mail" name="login-form-mail" value="" class="form-control" />
											</div>

											<div class="col_full">
												<label for="login-form-password">Mot de Passe:</label>
												<input type="password" id="login-form-password" name="login-form-password" value="" class="form-control" />
											</div>

											<div class="col_full nobottommargin">
												<button type="submit" class="button button-3d button-black nomargin" id="login-form-submit" name="login-form-submit" value="login">Connexion</button>
											</div>

										</form>
									</div>
								</div>
							</div>



						</div>

					</div>

				</div>

			</div>

		</section><!-- #content end -->



  <!-- Footer
		============================================= -->
      <% include tools/footer.ejs %>
